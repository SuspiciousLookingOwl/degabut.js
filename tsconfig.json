{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "useDefineForClassFields": true,
    "paths": {
      "@api": ["./src/api"],
      "@api/*": ["./src/api/*"],
      "@auth": ["src/auth"],
      "@auth/*": ["src/auth/*"],
      "@apps": ["./src/apps"],
      "@apps/*": ["./src/apps/*"],
      "@common": ["./src/common"],
      "@common/*": ["./src/common/*"],
      "@database": ["./src/database"],
      "@database/*": ["./src/database/*"],
      "@discord-bot": ["./src/discord-bot"],
      "@discord-bot/*": ["./src/discord-bot/*"],
      "@events": ["./src/events"],
      "@events/*": ["./src/events/*"],
      "@health": ["./src/health"],
      "@health/*": ["./src/health/*"],
      "@history": ["./src/history"],
      "@history/*": ["./src/history/*"],
      "@logger": ["./src/logger"],
      "@logger/*": ["./src/logger/*"],
      "@playlist": ["./src/playlist"],
      "@playlist/*": ["./src/playlist/*"],
      "@queue": ["./src/queue"],
      "@queue/*": ["./src/queue/*"],
      "@queue-player": ["./src/queue-player"],
      "@queue-player/*": ["./src/queue-player/*"],
      "@user": ["./src/user"],
      "@user/*": ["./src/user/*"],
      "@youtube": ["./src/youtube"],
      "@youtube/*": ["./src/youtube/*"]
    }
  }
}
