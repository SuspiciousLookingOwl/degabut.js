{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/core/src",
  "monorepo": true,
  "root": "apps/core",
  "defaultLibraryPrefix": "@",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/core/tsconfig.app.json"
  },
  "projects": {
    "core": {
      "type": "application",
      "root": "apps/core",
      "entryFile": "main",
      "sourceRoot": "apps/core/src",
      "compilerOptions": {
        "tsConfigPath": "apps/core/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "history": {
      "type": "library",
      "root": "libs/history",
      "entryFile": "index",
      "sourceRoot": "libs/history/src",
      "compilerOptions": {
        "tsConfigPath": "libs/history/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "media-source": {
      "type": "library",
      "root": "libs/media-source",
      "entryFile": "index",
      "sourceRoot": "libs/media-source/src",
      "compilerOptions": {
        "tsConfigPath": "libs/media-source/tsconfig.lib.json"
      }
    },
    "playlist": {
      "type": "library",
      "root": "libs/playlist",
      "entryFile": "index",
      "sourceRoot": "libs/playlist/src",
      "compilerOptions": {
        "tsConfigPath": "libs/playlist/tsconfig.lib.json"
      }
    },
    "queue": {
      "type": "library",
      "root": "libs/queue",
      "entryFile": "index",
      "sourceRoot": "libs/queue/src",
      "compilerOptions": {
        "tsConfigPath": "libs/queue/tsconfig.lib.json"
      }
    },
    "queue-player": {
      "type": "library",
      "root": "libs/queue-player",
      "entryFile": "index",
      "sourceRoot": "libs/queue-player/src",
      "compilerOptions": {
        "tsConfigPath": "libs/queue-player/tsconfig.lib.json"
      }
    },
    "spotify": {
      "type": "library",
      "root": "libs/spotify",
      "entryFile": "index",
      "sourceRoot": "libs/spotify/src",
      "compilerOptions": {
        "tsConfigPath": "libs/spotify/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "library",
      "root": "libs/user",
      "entryFile": "index",
      "sourceRoot": "libs/user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user/tsconfig.lib.json"
      }
    },
    "youtube": {
      "type": "library",
      "root": "libs/youtube",
      "entryFile": "index",
      "sourceRoot": "libs/youtube/src",
      "compilerOptions": {
        "tsConfigPath": "libs/youtube/tsconfig.lib.json"
      }
    }
  }
}