{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "useDefineForClassFields": true,
    "resolveJsonModule": true,
    "paths": {
      // "@api": ["./src/apps/api"],
      // "@api/*": ["./src/apps/api/*"],
      // "@youtube-api": ["./src/apps/youtube-api"],
      // "@youtube-api/*": ["./src/apps/youtube-api/*"],
      // "@discord-bot": ["./src/apps/discord-bot"],
      // "@discord-bot/*": ["./src/apps/discord-bot/*"],
      "@events": ["./src/apps/events"],
      "@events/*": ["./src/apps/events/*"],
      "@common": ["libs/common"],
      "@common/*": ["libs/common/*"],
      "@auth": ["libs/auth/src"],
      "@auth/*": ["libs/auth/src/*"],
      "@apps": ["libs/apps/src"],
      "@apps/*": ["libs/apps/src/*"],
      "@database": ["libs/database/src"],
      "@database/*": ["libs/database/src/*"],
      "@health": ["libs/health/src"],
      "@health/*": ["libs/health/src/*"],
      "@history": ["libs/history/src"],
      "@history/*": ["libs/history/src/*"],
      "@logger": ["libs/logger/src"],
      "@logger/*": ["libs/logger/src/*"],
      "@playlist": ["libs/playlist/src"],
      "@playlist/*": ["libs/playlist/src/*"],
      "@queue": ["libs/queue/src"],
      "@queue/*": ["libs/queue/src/*"],
      "@queue-player": ["libs/queue-player/src"],
      "@queue-player/*": ["libs/queue-player/src/*"],
      "@media-source": ["libs/media-source/src"],
      "@media-source/*": ["libs/media-source/src/*"],
      "@spotify": ["libs/spotify/src"],
      "@spotify/*": ["libs/spotify/src/*"],
      "@user": ["libs/user/src"],
      "@user/*": ["libs/user/src/*"],
      "@youtube": ["libs/youtube/src"],
      "@youtube/*": ["libs/youtube/src/*"]
    }
  }
}
